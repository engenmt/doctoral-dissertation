	journal = {Nucleic Acids Research},
	number = {6},
	pages = {1929-1946},
	title = {The {tRNA}-{U}ke structure at the $3'$ terminus of turnip yellow mosaic virus {RNA}. Differences and similarities with canonical {tRNA}},
